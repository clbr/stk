<?xml version="1.0"?>
<particles emitter="point">

    <spreading angle="0"      />
    
    <velocity  x="0.000"
               y="0.0002"
               z="-0.005"         />
    
    <materials>
        <material name="waterparticles0-subsea.png" compositing="blend"/>
    </materials>
    
    <!-- Amount of particles emitted per second -->
    <rate      min="15"
               max="25"         />
    
    <!-- Minimal and maximal lifetime of a particle, in milliseconds. -->
    <lifetime  min="200"
               max="1000"         />
    
    <!-- Size of the particles -->
    <size      min="0.50"
               max="0.80"        />
    
    <color     min="255 255 255"
               max="255 255 255" />

    <fadeout   time="300" />


</particles>

</particles>
